    <LangVersion>latest</LangVersion>
    <Version>0.0.1.0-alpha1</Version>
    <Authors>Алексей Якубин</Authors>
    <Company>Делобайты</Company>
    <Copyright>Авторские права © Делобайты 2022. Все права защищены</Copyright>
    <Description>Утилиты инверсии зависимостей</Description>
    <RepositoryUrl>https://github.com/a-postx/Delobytes.NetCore.DependencyInjection.git</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Nullable>enable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
